[
    {
        "method": "get",
        "path": "/",
        "controller": "home.index",
        "middlewares": [
            "auth.validate"
        ],"roles":["admin"]
    },
    {
        "method": "get",
        "path": "/products",
        "controller": "product.index",
        "middlewares": [
            "auth.kick","auth.validate"
        ],"roles":[]
    },
    {
        "method": "post",
        "path": "/products",
        "controller": "product.add",
        "middlewares": [
            "auth.validate"
        ],"roles":[]
    } ,
    {
        "method":"put",
        "path":"/products",
        "controller":"product.update",
        "middlewares": [
            "auth.validate"
        ],"roles":[]
    },
    {
        "method": "get",
        "path": "/products/:id",
        "controller": "product.getProduct",
        "middlewares": [
            "auth.kick","auth.validate"
        ],"roles":[]       
    },{
        "method":"delete",
        "path":"/products/:id",
        "controller":"product.deleteProduct",
        "middlewares":["auth.validate"],
        "roles":[]
    }  ,{
        "method":"post",
        "path":"/register",
        "controller":"user.register",
        "middlewares":["auth.validate"],
        "roles":[]
    },{
        "method":"post",
        "path":"/login",
        "controller":"user.login",
        "middlewares":["auth.validate"],
        "roles":[]
    },{
        "method":"post",
        "path":"/refresh-token",
        "controller":"user.refreshToken",
        "middlewares":["auth.validate"],
        "roles":[]
    }
]